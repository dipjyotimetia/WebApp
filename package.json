{
  "name": "reactweb",
  "version": "2.0.0",
  "private": true,
  "author": "Dipjyoti Metia",
  "scripts": {
    "webpack:start": "webpack-dev-server --config webpack.config.dev.js --open",
    "webpack:build": "webpack --config webpack.config.prod.js",
    "start": "run-p start:dev start:api",
    "start:dev": "cross-env REACT_APP_API_URL=http://localhost:3001 react-scripts start",
    "prestart:api": "node tools/createMockDb.js",
    "start:api": "node tools/apiServer.js",
    "build": "react-scripts build",
    "test": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "eject": "react-scripts eject",
    "lint": "eslint --cache --format codeframe --ext js,jsx src",
    "lint:fix": "eslint --cache --format codeframe --ext js,jsx src --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "7.6.0",
    "bootstrap": "^4.3.1",
    "chalk": "2.4.2",
    "compression": "1.7.4",
    "lodash": "4.17.21",
    "minimist": "1.2.6",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^5.4.0",
    "redux": "4.0.4",
    "redux-saga": "1.1.1",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "autoprefixer": "^9.7.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-redux-saga": "1.1.1",
    "eslint-plugin-react-hooks": "2.2.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "11.1.1",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0",
    "svg-url-loader": "^3.0.2",
    "url-loader": "^2.2.0",
    "terser-webpack-plugin": "^2.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "~3.2.1",
    "webpack-merge": "^4.2.2",
    "cross-env": "^5.2.0",
    "json-server": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "ngrok": "3.2.5",
    "react-test-renderer": "^16.11.0",
    "react-testing-library": "8.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn lint"
    ]
  }
}
